{
  "name": "checkout",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "check": "npm-check --skip-unused",
    "start": "node .",
    "start:build": "npm-run-all -s clean build start:simple",
    "start:simple": "node .",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "build": "npm-run-all -s build:ts",
    "build:ts": "tsc",
    "generate:clients": "openapi-generator generate -g typescript-node -i ../orders/openapi.yml -o ./src/clients/orders --skip-validate-spec --additional-properties supportsES6=true",
    "watch:ts": "tsc --watch",
    "watch:node": "nodemon --debug --delay 2.5",
    "lint": "tslint ./src/{,**/}*.ts",
    "pretest": "npm-run-all -s clean build",
    "test": "jest --coverage ./src/tests",
    "test:api": "jest --coverage ./src/tests --testMatch '**/src/tests/api/**'",
    "test:unit": "jest --coverage ./src/tests --testMatch '**/src/tests/unit/**'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.6.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^2.1.1",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "event-dispatch": "^0.4.1",
    "express": "^4.17.1",
    "express-ping": "^1.4.0",
    "express-prometheus-middleware": "^0.9.6",
    "express-winston": "^4.0.5",
    "fetch": "^1.1.0",
    "form-data": "^3.0.0",
    "glob": "^7.1.6",
    "handy-redis": "^1.8.3",
    "js-yaml": "^3.14.0",
    "prom-client": "^12.0.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "routing-controllers": "^0.8.1",
    "routing-controllers-openapi": "^2.1.0",
    "rxjs": "^6.6.3",
    "source-map-support": "^0.5.19",
    "swagger-client": "^3.11.1",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^4.1.4",
    "ts-helpers": "^1.1.2",
    "typedi": "^0.8.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async-redis": "^1.1.1",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.13",
    "@types/config": "0.0.36",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/glob": "^7.1.3",
    "@types/jasmine": "^3.5.14",
    "@types/jest": "^26.0.14",
    "@types/js-yaml": "^3.12.5",
    "@types/jsdom": "^16.2.4",
    "@types/node": "^14.11.5",
    "@types/redis": "^2.8.27",
    "@types/serve-static": "^1.13.5",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "jest": "^26.5.2",
    "jest-when": "^3.1.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^5.0.0",
